fib = func (n) : f {
    if (n == 0)
        0;
    else if (n <= 2)
        1;
    else
        f(n - 2) + f(n - 1);
}

step = 0;
count_steps = 10;
while (step < count_steps) {
    print fib(step);
    step = step + 1;
}
